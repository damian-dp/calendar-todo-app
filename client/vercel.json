{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [{ "key": "Content-Type", "value": "image/x-icon" }]
    },
    {
      "source": "/(favicon-16x16|favicon-32x32).png",
      "headers": [{ "key": "Content-Type", "value": "image/png" }]
    },
    {
      "source": "/apple-touch-icon.png",
      "headers": [{ "key": "Content-Type", "value": "image/png" }]
    }
  ]
}